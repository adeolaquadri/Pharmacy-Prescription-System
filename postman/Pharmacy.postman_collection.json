{
  "info": {
    "name": "Pharmacy",
    "_postman_id": "pharmacy-collection",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Auth - Login",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": {
          "raw": "http://localhost:4000/auth/login",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "4000",
          "path": [
            "api",
            "auth",
            "login"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\"email\": \"admin@example.com\", \"password\": \"Admin@123\"}"
        }
      }
    },
    {
      "name": "Patients - Create",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{token}}"
          }
        ],
        "url": {
          "raw": "http://localhost:4000/patients",
          "protocol": "http",
          "host": [
            "localhost"
          ],
          "port": "4000",
          "path": [
            "api",
            "patients"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\"name\": \"Alice\", \"email\": \"alice@example.com\", \"phone\": \"08030000003\", \"dateOfBirth\": \"1995-09-09\"}"
        }
      }
    }
  ],
  "variable": [
    {
      "key": "token",
      "value": ""
    }
  ]
}